<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.treasure.recycler_view">

    <application
        android:name=".TAPP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.VerticalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.VerDelReportActivity" />
        <activity android:name=".activity.VerGridActivity" />
        <activity android:name=".activity.HorVerActivity" />
        <activity android:name=".activity.WaterFallActivity"></activity>
    </application>

</manifest>